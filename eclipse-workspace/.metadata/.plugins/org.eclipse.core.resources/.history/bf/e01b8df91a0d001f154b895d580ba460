#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int limit,i,j;
	print("enter a limit");
	scanf("%d"&limit);
	for(i=1;i<=limit;i++){
		for(j=1;j<=i;j++){
			print("%d",j);
		}print("\n");
	}
	return EXIT_SUCCESS;
}
